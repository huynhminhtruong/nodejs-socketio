<div class="container-fluid">
  <div class="row">
    <form id="sign-up-form" action="/users" method="POST">
      <table class="table table-hover">
        <tr>
          <td style="text-align: center;" colspan="<%= images.length%>">
            <img src="<%= images[0].image%>" class="user-avatar">
          </td>
        </tr>
        <% for(var i = 1; i < images.length; i++) {%>
        <td style="text-align: center;">
        <img src="<%= images[i].image %>" class="user-avatar">
      </td>
        <% } %>
      </table>
    </form>
  </div>
</div>
<div class="row" style="margin-top: -50px; margin-bottom: 35px;">
  <div class="col-md-3"></div>
    <div class="col-md-6">
      <h2>Your List Images</h2>
    </div>
  <div class="col-md-3"></div>
  </div>
  <div class="row">
    <div class="col-md-3"></div>
      <div class="col-md-6">
        <form id="sign-up-form" action="/users" method="POST">
          <table class="table table-hover">
            <% for(var i = 0; i <= images.length - 1; i+=2) {%>
              
            <tr>
              <td><img src="<%= images[i].image %>" class="user-avatar"></td>
              <% if(i < images.length - 1) { %>
              <td><img src="<%= images[i + 1].image %>" class="user-avatar"></td>
              <% } %>
            </tr>
              
            <% } %>
          </table>
      </form>
    </div>
  <div class="col-md-3"></div>
</div>